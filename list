 #include <stdio.h>
 typedef struct node 
 {
 	int entry;
 	struct node*next;
 }node;
 int main()
 {
 	int n; //the numberof elements
 	int count; //number of list nodes
 	int element; //the elements in the list
 	node *newnode; //the new node
 	
 	printf("enter the number of nodes in the list");
 	scanf("%d",&n);
 	printf("enter the element in the node");
 	scanf("%d",&element);
 	newnode->entry=element;
 	printf("%d",newnode->entry);
 }
